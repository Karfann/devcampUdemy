<h1>Portifolio Items</h1>

<%= link_to "Create new item", new_portifolio_path %>

<% @portifolio_items.each do |portifolio_item|%>
    <p><%= portifolio_item.title %></p>
    <p><%= portifolio_item.subtitle %></p>
    <p><%= portifolio_item.body %></p>
    <%= image_tag portifolio_item.thumb_image unless portifolio_item.thumb_image.nil? %>
    <%= link_to "Edit", edit_portifolio_path(portifolio_item.id) %>
<% end %>